{
  "name": "lnd-binary",
  "version": "0.2.1",
  "description": "Download the latest lnd binary",
  "main": "./bin/lnd",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.0.0"
  },
  "scripts": {
    "install": "node scripts/install.js",
    "prepublishOnly": "node scripts/prepublish.js",
    "test": "tape test/*.js | tap-spec",
    "lint": "prettier-eslint --write \"./**/*.{js,json,md}\""
  },
  "files": [
    "bin",
    "lib",
    "scripts",
    "src",
    "test",
    "vendor"
  ],
  "bin": {
    "lnd": "./bin/lnd",
    "lnd-install": "./bin/lnd-install"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrfelton/lnd-binary.git"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cacache": "^11.0.2",
    "debug": "^3.1.0",
    "find-config": "^1.0.0",
    "go-platform": "^1.0.0",
    "gunzip-maybe": "^1.4.1",
    "hasha": "^3.0.0",
    "mkdirp": "^0.5.1",
    "npmlog": "^4.1.2",
    "rimraf": "^2.6.2",
    "tar-fs": "^1.16.2",
    "true-case-path": "^1.0.2",
    "unzip-stream": "^0.3.0"
  },
  "keywords": [
    "bitcoin",
    "lightning-network",
    "lnd"
  ],
  "author": "Tom Kirkpatrick <tkp@kirkdesigns.co.uk> (http://www.kirkdesigns.co.uk/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrfelton/lnd-binary/issues"
  },
  "homepage": "https://github.com/mrfelton/lnd-binary",
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "prettier": "^1.13.5",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.0"
  }
}
